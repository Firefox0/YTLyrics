{

    "manifest_version": 2,
    "name": "YTLyrics",
    "version": "0.3",
  
    "description": "Adds lyrics to the youtube description.",
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["ytlyrics.js"]
      }
    ],

    "permissions": [
      "*://*.youtube.com/*",
      "*://api.genius.com/*",
      "*://genius.com/*"
    ]
  
  }
  